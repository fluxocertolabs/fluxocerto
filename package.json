{
  "name": "family-finance",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm db:start && pnpm dev:app",
    "dev:app": "vite",
    "db:start": "npx supabase start",
    "db:stop": "npx supabase stop",
    "db:reset": "npx supabase db reset",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest --watch",
    "test:unit:coverage": "vitest --coverage",
    "test:e2e": "pnpm test:e2e:setup && playwright test --config e2e/playwright.config.ts",
    "test:e2e:ui": "pnpm test:e2e:setup && playwright test --config e2e/playwright.config.ts --ui",
    "test:e2e:debug": "pnpm test:e2e:setup && playwright test --config e2e/playwright.config.ts --debug",
    "test:e2e:report": "playwright show-report",
    "test:e2e:setup": "npx supabase status > /dev/null 2>&1 || npx supabase start",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "2.86.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "2.1.1",
    "date-fns": "4.1.0",
    "lucide-react": "0.468.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-router-dom": "7.9.6",
    "recharts": "3.5.0",
    "tailwind-merge": "3.4.0",
    "zod": "4.1.13",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@playwright/test": "1.57.0",
    "@tailwindcss/vite": "4.1.17",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "24.10.1",
    "@types/pg": "8.15.4",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "autoprefixer": "10.4.22",
    "eslint": "9.39.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "husky": "^9.1.7",
    "jsdom": "27.2.0",
    "pg": "8.16.0",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.17",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.0",
    "vite": "7.2.4",
    "vitest": "4.0.14"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.12.1"
}
