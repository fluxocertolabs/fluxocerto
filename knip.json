{
  "$schema": "https://unpkg.com/knip@5.82.1/schema.json",
  "workspaces": {
    ".": {
      "entry": [
        "src/main.tsx",
        "src/App.tsx",
        "scripts/*.ts",
        "e2e/playwright.config.ts",
        "e2e/**/*.spec.ts",
        "e2e/**/*.smoke.ts",
        "supabase/functions/*/index.ts"
      ],
      "project": [
        "src/**/*.{ts,tsx}",
        "scripts/**/*.ts",
        "e2e/**/*.{ts,tsx}",
        "supabase/functions/**/*.{ts,tsx}"
      ],
      "ignore": [
        "**/node_modules/**",
        "**/dist/**",
        "**/coverage/**",
        "**/test-results/**",
        "**/playwright-report/**",
        "**/.pnpm-store/**",
        "**/e2e/tests/visual/**/*-snapshots/**",
        "**/.specify/**"
      ],
      "ignoreBinaries": [
        "supabase"
      ]
    }
  },
  "exclude": [
    "binaries",
    "unlisted"
  ],
  "ignoreBinaries": [
    "supabase"
  ]
}

